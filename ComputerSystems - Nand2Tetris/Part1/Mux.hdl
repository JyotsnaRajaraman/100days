/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out =notsel);
    Nand(a=a, b=notsel, out =aout);
    Nand(a=sel, b=b, out =bout);
    Nand(a=aout, b=bout, out =out);
}